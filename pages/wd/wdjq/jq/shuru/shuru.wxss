.swiper_wrap{
  position: absolute;
  width: 80%;
  height: 80%;
  left: 50%;
  top: 30rpx;
  transform: translateX(-50%);
  border: 4rpx solid #ccc;
  border-radius: 15rpx;
  padding: 30rpx;
}

.swiper-block {
  position: relative;
  height: 80%;
  width: 100%;
  margin-bottom: 80rpx;
}

.slide-image {
  height: 100%;
  width: 100%;
}

.imageCount {
  width:120rpx;
  height:50rpx;
  background-color: rgba(0, 0, 0, 0.3);
  border-radius:40rpx;
  line-height:50rpx;
  color:#fff;
  text-align:center;
  font-size:26rpx;
  position:absolute;
  right: 26rpx;
  bottom:20rpx;
}