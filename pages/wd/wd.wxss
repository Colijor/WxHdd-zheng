page{background-color: #f0f0f0;}
/* 授权页面 */
.authorizeHeader {
    margin: 0 0 50rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
 
.authorizeHeader image {
    width: 200rpx;
    height: 200rpx;
    margin: 0 !important;
}
 
.authorizeContent {
    margin-left: 50rpx;
    margin-right: 50rpx;
    margin-bottom: 90rpx;
}
.authorizeInfo{
  display: flex;
  align-items: flex-start;
  margin-top: 50rpx;
}
.authorizeInfo image{
  width: 50rpx;
  height: 50rpx;
  margin: 0 !important;
}
.authorizeInfo text {
    display: inline-block;
    margin-left: 20rpx;
    font-size: 30rpx;
}
 
.authorizeBottom {
    text-align: center;
    margin-top: 300rpx;
}
.authorizeBottom button{
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
}
/* 扫一扫  */

.wd .top .ewn image{
    height: 60rpx;
    width: 60rpx;
    position: absolute;
    left: 650rpx;
    top: 30rpx;
}
/* 头像昵称  */
.wd .top{background-color: #Da251D;height: 430rpx;}
.top{position: relative;}
.header{position: absolute;left: 50%;top:40%;transform:translate(-50%,-50%);text-align: center;}
.headImg{width: 150rpx;height: 150rpx;border-radius:50%;overflow:hidden;display: block}
.headNick{text-align: center;font-size: 35rpx;color: #f0f0f0;}
.search{
  position: absolute;
  left: 50%;
  top: 300rpx;
  transform:translateX(-50%);
  /* width: 360rpx; */
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
}
.bar{
  background-color: #fff;
  border-radius: 15rpx;
}
input{
  float: left;
  width: 250rpx;
  height: 70rpx;
  padding-left: 30rpx;
}
.search button{
  width: 120rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #fff;
  color: #Da251D;
  text-align: center;
  border-radius: 15rpx;
  margin-left: 10rpx;
  padding: 0 10rpx;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
.icon {
  font-family: "iconfont" !important;
  font-size: 50rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color: #Da251D;
}

.icon-search:before {
  content: "\e641";
}


